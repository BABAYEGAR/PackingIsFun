@{
    ViewData["Title"] = "Home Page";
}
<!--Banner-->
<section class="banner">

<!--Background-->
<div class="bg-parallax bg-1"></div>
<!--End Background-->

<div class="container">

<div class="logo-banner text-center">
    <a href="" title="">
        <img src="~/LandingPage/images/logo-banner.png" alt="">
    </a>
</div>

<!-- Banner Content -->
<div class="banner-cn">

<!-- Tabs Content -->
<div class="tab-content">
<!-- Search Flight-->
<div class="form-cn form-flight tab-pane active">
    <h2>Where would you like to go?</h2>
    <div class="form-search clearfix">
        <div class="form-field field-from">
            <label for="flight-from"><span>Pick Up Location:</span> School</label>
            <input type="text" name="flightfrom" class="field-input">
        </div>
        <div class="form-field field-from">
            <label for="flight-from"><span>No. Boxes:</span></label>
            <input type="text" name="flightfrom" class="field-input">
        </div>
        <div class="form-field field-date">
            <input type="text" class="field-input calendar-input" placeholder="Start Date">
        </div>
        <div class="form-field field-date">
            <input type="text" class="field-input calendar-input" placeholder="End Date">
        </div>
        <div class="form-field field-select field-adult">
            <div class="select">
                <span>Adults</span>
                <select>
                    <option>Adults</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                </select>
            </div>
        </div>
        <div class="form-field field-select field-children">
            <div class="select">
                <span>Children</span>
                <select>
                    <option>Children</option>
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                </select>
            </div>
        </div>
        <div class="form-submit">
            <button type="submit" class="awe-btn awe-btn-medium awe-search">Search</button>
        </div>
    </div>
</div>
<!-- End Search Flight -->


</div>
<!-- End Tabs Content -->

</div>
<!-- End Banner Content -->

</div>

</section>
<!--End Banner
