<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <!-- Font Google -->
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400%7COpen+Sans:300,400,600" rel="stylesheet" type="text/css">
    <!-- End Font Google -->
    <!-- Library CSS -->
    <link rel="stylesheet" href="~/LandingPage/css/library/font-awesome.min.css">
    <link rel="stylesheet" href="~/LandingPage/css/library/bootstrap.min.css">
    <link rel="stylesheet" href="~/LandingPage/css/library/jquery-ui.min.css">
    <link rel="stylesheet" href="~/LandingPage/css/library/owl.carousel.css">
    <link rel="stylesheet" href="~/LandingPage/css/library/jquery.mb.YTPlayer.min.css">
    <!-- End Library CSS -->
    <link rel="stylesheet" href="~/LandingPage/css/style.css">
</head>
<body>
    <!-- Preloader -->
    <div id="preloader">
        <div class="tb-cell">
            <div id="page-loading">
                <div></div>
                <p>Loading</p>
            </div>
        </div>
    </div>
    <!-- Wrap -->
    <div id="wrap">

        @{
            Html.RenderPartial("_LandingHeader");
        }
        @RenderBody()

        @{
            Html.RenderPartial("_LandingFooter");
        }


    </div>

    <!-- Library JS -->
    <script type="text/javascript" src="~/LandingPage/js/library/jquery-1.11.0.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/parallax.min.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/jquery.nicescroll.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/jquery.ui.touch-punch.min.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/jquery.mb.YTPlayer.min.js"></script>
    <script type="text/javascript" src="~/LandingPage/js/library/SmoothScroll.js"></script>
    <!-- End Library JS -->
    <!-- Main Js -->
    <script type="text/javascript" src="~/LandingPage/js/script.js"></script>
    <!-- End Main Js -->
</body>
</html>